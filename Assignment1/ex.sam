start:
	PUSHIMM 0
	LINK
	JSR main
	POPFBR
	STOP

fact:
	PUSHOFF -1
	PUSHIMM 0
	EQUAL
	JUMPC ifLbl0
	PUSHOFF -1
	PUSHIMM 0
	PUSHOFF -1
	PUSHIMM 1
	SUB
	LINK
	JSR fact
	POPFBR
	ADDSP -1
	TIMES
	STOREOFF -2
	ADDSP -0
	JUMPIND
	JUMP ifEndLbl0
ifLbl0:
	PUSHIMM 1
	STOREOFF -2
	ADDSP -0
	JUMPIND
ifEndLbl0:

main:
	PUSHIMM 0
	PUSHIMM 4
	LINK
	JSR fact
	POPFBR
	ADDSP -1
	STOREOFF -1
	ADDSP -0
	JUMPIND


